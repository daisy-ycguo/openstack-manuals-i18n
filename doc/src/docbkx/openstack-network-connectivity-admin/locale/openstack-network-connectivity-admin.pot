msgid "Right-click the <emphasis role=\"strong\">Computer</emphasis> menu item."
msgstr ""

#: /home/daisy/OpenStack/openstack-manuals-i18n/doc/src/docbkx/openstack-api-programming/openstackapi-programming.xml:305(para)
msgid "Select <emphasis role=\"strong\">Properties</emphasis>."
msgstr ""

#: /home/daisy/OpenStack/openstack-manuals-i18n/doc/src/docbkx/openstack-api-programming/openstackapi-programming.xml:310(para)
msgid "Select <emphasis role=\"strong\">Advanced System Settings</emphasis>."
msgstr ""

#: /home/daisy/OpenStack/openstack-manuals-i18n/doc/src/docbkx/openstack-api-programming/openstackapi-programming.xml:316(para)
msgid "Click the <emphasis role=\"strong\">Environment Variables</emphasis> button."
msgstr ""

#: /home/daisy/OpenStack/openstack-manuals-i18n/doc/src/docbkx/openstack-api-programming/openstackapi-programming.xml:322(para)
msgid "Click the <emphasis role=\"strong\">Path</emphasis> entry on the <emphasis role=\"strong\">System Variables</emphasis> list."
