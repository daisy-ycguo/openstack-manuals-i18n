#, no-wrap
msgid "uid = swift\ngid = swift\nlog file = /var/log/rsyncd.log\npid file = /var/run/rsyncd.pid\naddress = &lt;STORAGE_LOCAL_NET_IP&gt;\n\n[account]\nmax connections = 2\npath = /srv/node/\nread only = false\nlock file = /var/lock/account.lock\n\n[container]\nmax connections = 2\npath = /srv/node/\nread only = false\nlock file = /var/lock/container.lock\n\n[object]\nmax connections = 2\npath = /srv/node/\nread only = false\nlock file = /var/lock/object.lock"
