#: /home/daisy/OpenStack/openstack-manuals-i18n/doc/src/docbkx/openstack-install/object-storage-howto-install-multinode.xml:351(screen)
#, no-wrap
msgid "\nfdisk /dev/sdb  (set up a single partition)\nmkfs.xfs -i size=1024 /dev/sdb1\necho \"/dev/sdb1 /srv/node/sdb1 xfs noatime,nodiratime,nobarrier,logbufs=8 0 0\" &gt;&gt; /etc/fstab\nmkdir -p /srv/node/sdb1\nmount /srv/node/sdb1\nchown -R swift:swift /srv/node\n"
msgstr ""

#: /home/daisy/OpenStack/openstack-manuals-i18n/doc/src/docbkx/openstack-install/object-storage-howto-install-multinode.xml:361(para) /home/daisy/OpenStack/openstack-manuals-i18n/doc/src/docbkx/openstack-install/install-config-storage-nodes.xml:31(para)
msgid "Create /etc/rsyncd.conf:"
msgstr ""

#: /home/daisy/OpenStack/openstack-manuals-i18n/doc/src/docbkx/openstack-install/object-storage-howto-install-multinode.xml:364(screen)
#, no-wrap
msgid "\ncat &gt;/etc/rsyncd.conf &lt;&lt;EOF\nuid = swift\ngid = swift\nlog file = /var/log/rsyncd.log\npid file = /var/run/rsyncd.pid\naddress = $STORAGE_LOCAL_NET_IP\n\n[account]\nmax connections = 2\npath = /srv/node/\nread only = false\nlock file = /var/lock/account.lock\n\n[container]\nmax connections = 2\npath = /srv/node/\nread only = false\nlock file = /var/lock/container.lock\n\n[object]\nmax connections = 2\npath = /srv/node/\nread only = false\nlock file = /var/lock/object.lock\nEOF\n"
msgstr ""

#: /home/daisy/OpenStack/openstack-manuals-i18n/doc/src/docbkx/openstack-install/object-storage-howto-install-multinode.xml:393(para)
msgid "Edit the RSYNC_ENABLE= line in /etc/default/rsync:"
msgstr ""

#: /home/daisy/OpenStack/openstack-manuals-i18n/doc/src/docbkx/openstack-install/object-storage-howto-install-multinode.xml:396(screen)
#, no-wrap
msgid "\nperl -pi -e 's/RSYNC_ENABLE=false/RSYNC_ENABLE=true/' /etc/default/rsync\n"
msgstr ""

#: /home/daisy/OpenStack/openstack-manuals-i18n/doc/src/docbkx/openstack-install/object-storage-howto-install-multinode.xml:401(para) /home/daisy/OpenStack/openstack-manuals-i18n/doc/src/docbkx/openstack-install/install-config-storage-nodes.xml:60(para)
msgid "Start rsync daemon:"
msgstr ""

#: /home/daisy/OpenStack/openstack-manuals-i18n/doc/src/docbkx/openstack-install/object-storage-howto-install-multinode.xml:404(screen)
#, no-wrap
msgid "\nservice rsync start\n"
msgstr ""

#: /home/daisy/OpenStack/openstack-manuals-i18n/doc/src/docbkx/openstack-install/object-storage-howto-install-multinode.xml:409(para) /home/daisy/OpenStack/openstack-manuals-i18n/doc/src/docbkx/openstack-install/install-config-storage-nodes.xml:64(para)
msgid "The rsync daemon requires no authentication, so it should be run on a local, private network."
msgstr ""

#: /home/daisy/OpenStack/openstack-manuals-i18n/doc/src/docbkx/openstack-install/object-storage-howto-install-multinode.xml:412(para) /home/daisy/OpenStack/openstack-manuals-i18n/doc/src/docbkx/openstack-install/install-config-storage-nodes.xml:68(para)
msgid "Create /etc/swift/account-server.conf:"
msgstr ""

#: /home/daisy/OpenStack/openstack-manuals-i18n/doc/src/docbkx/openstack-install/object-storage-howto-install-multinode.xml:415(screen)
#, no-wrap
msgid "\ncat &gt;/etc/swift/account-server.conf &lt;&lt;EOF\n[DEFAULT]\nbind_ip = $STORAGE_LOCAL_NET_IP\nworkers = 2\n\n[pipeline:main]\npipeline = account-server\n\n[app:account-server]\nuse = egg:swift#account\n\n[account-replicator]\n\n[account-auditor]\n\n[account-reaper]\nEOF\n"
