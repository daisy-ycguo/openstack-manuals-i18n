#, no-wrap
msgid "\n$ curl -s -X POST -H \"X-Auth-Token:999888777666\" -d '{\"changePassword\": {\"adminPass\": \"dontusesshhhforpassword\"}}' http://localhost:8774/v1.1/openstack/servers/3/action\n"
msgstr ""

