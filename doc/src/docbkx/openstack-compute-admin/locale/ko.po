#: /home/daisy/OpenStack/openstack-manuals-i18n/doc/src/docbkx/openstack-compute-admin/computeconfigure.xml484(title)
msgid "XenServer 5.6, Flat networking, MySQL, and Glance, OpenStack API"
msgstr ""

#: /home/daisy/OpenStack/openstack-manuals-i18n/doc/src/docbkx/openstack-compute-admin/computeconfigure.xml487(para)
msgid ""
"This example <filename>nova.conf</filename> file is from an internal "
"Rackspace test system."
msgstr ""

#: /home/daisy/OpenStack/openstack-manuals-i18n/doc/src/docbkx/openstack-compute-admin/computeconfigure.xml490(programlisting)
#, no-wrap
msgid ""
"\n"
"verbose\n"
"nodaemon\n"
"sql_connection=mysql://root:&lt;password&gt;@127.0.0.1/nova\n"
"network_manager=nova.network.manager.FlatManager\n"
"image_service=nova.image.glance.GlanceImageService\n"
"flat_network_bridge=xenbr0\n"
"connection_type=xenapi\n"
"xenapi_connection_url=https://&lt;XenServer IP&gt;\n"
"xenapi_connection_username=root\n"
"xenapi_connection_password=supersecret\n"
