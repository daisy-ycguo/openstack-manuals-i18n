#, no-wrap
msgid ""
"\n"
"$ curl -X 'POST' -v https://nova-api.trystack.org:5443/v2.0/tokens -d '{\"auth\":{\"passwordCredentials\":{\"username\": \"joecool\", \"password\":\"coolword\"}, \"tenantId\":\"5\"}}' -H 'Content-type: application/json'\n"
"         "
msgstr ""

